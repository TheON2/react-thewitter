{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "import",
    "react-hooks"
  ],
  "rules": {
    "linebreak-style": "off",
    "jsx-a11y/label-had-associated-control": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "no-console": "off",
    "no-undenscore-dangle": "off",
    "react/forbid-prop-types": "off",
    "react/jsx-filename-extention": "off",
    "react/jsx-one-expression-per-line": "off",
    "object-curly-newline": "off",
    "no-param-reassign": "off",
    "import/no-unresolved": "off"
  }
}